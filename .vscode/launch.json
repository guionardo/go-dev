  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Package",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "args": ["go", "escoteirando-chefia"]
    },
    {
      "name": "Setup escoteirando",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "args": [
        "setup",
        "-f",
        "/home/guionardo/dev/github.com/guionardo/escoteirando-chefia",
        "--command",
        "vscode"
      ]
    },
    {
      "name": "Setup add dev",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "args": [
        "-c",
        "./go-dev-dev.yaml",
        "setup",
        "add-folder",
        "/home/guionardo/dev"
      ]
    },
    {
      "name": "Sync",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "args": ["-c", "./go-dev-dev.yaml", "sync"]
    },
    {
      "name": "Setup autosync",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "args": ["-c", "./go-dev-dev.yaml", "setup", "auto-sync", "-i", "5"]
    },
    {
      "name": "Go",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "args": ["-c", "./go-dev-dev.yaml", "go", "estoque"]
    },
    {
      "name": "Init",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "args": ["init"]
    }
  ]
}
